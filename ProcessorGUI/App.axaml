<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:ProcessorGUI" 
             xmlns:viewModels="clr-namespace:ProcessorGUI.ViewModels" 
             xmlns:views="clr-namespace:ProcessorGUI.Views"
             x:Class="ProcessorGUI.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
		<DataTemplate DataType="{x:Type viewModels:ImageViewModel}">
			<views:ImageView/>
		</DataTemplate>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme Mode="Dark"/>
    </Application.Styles>

</Application>
